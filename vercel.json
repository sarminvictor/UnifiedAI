{
  "version": 2,
  "buildCommand": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' next build --no-lint",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
} 