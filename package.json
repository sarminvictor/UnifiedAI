{
  "name": "unifiedai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000 --hostname 0.0.0.0",
    "start-dev": "sh scripts/start-dev.sh",
    "build": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' next build || NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' next build --no-cache || NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' next build --no-lint --no-mangling",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "seed": "ts-node prisma/seed.ts",
    "run-prisma-migrate": "npx prisma migrate dev",
    "run-prisma-migrate-name": "npx prisma migrate dev --name <migration_name>",
    "prisma-generate": "npx prisma generate",
    "prisma-studio": "npx prisma studio",
    "start-prod": "npm run build && npm run start",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:regression": "jest tests/regression --config jest.config.js --runInBand",
    "test:regression:watch": "jest tests/regression --config jest.config.js --watch --runInBand",
    "postinstall": "prisma generate",
    "migrate-chat-ids": "ts-node scripts/migrateChatIds.ts",
    "db:clear": "ts-node prisma/clear.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
