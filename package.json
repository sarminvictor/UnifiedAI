{
  "name": "unifiedai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000 --hostname 0.0.0.0",
    "start-dev": "sh scripts/start-dev.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "seed": "ts-node prisma/seed.ts",
    "run-prisma-migrate": "npx prisma migrate dev",
    "run-prisma-migrate-name": "npx prisma migrate dev --name <migration_name>",
    "prisma-generate": "npx prisma generate",
    "prisma-studio": "npx prisma studio",
    "start-prod": "npm run build && npm run start",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:regression": "jest tests/regression --config jest.config.js --runInBand",
    "test:regression:watch": "jest tests/regression --config jest.config.js --watch --runInBand"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@geist-ui/core": "^2.3.8",
    "@langchain/anthropic": "^0.3.13",
    "@langchain/community": "^0.3.30",
    "@langchain/deepseek": "^0.0.1",
    "@langchain/google-genai": "^0.1.8",
    "@langchain/openai": "^0.4.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.3.1",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "critters": "^0.0.23",
    "dotenv": "^16.4.7",
    "langchain": "^0.3.15",
    "next": "^15.1.6",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.0",
    "react": "^18",
    "react-dom": "^18",
    "resend": "^4.1.2",
    "swr": "^2.3.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.17.17",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.1",
    "prettier": "^3.5.0",
    "prisma": "^6.3.1",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
